{
  "name": "@zapier/service",
  "private": true,
  "version": "1.0.0",
  "description": "zhwdailysummarizer nextjs service",
  "license": "UNLICENSED",
  "repository": "https://gitlab.com/zapier/zhwdailysummarizer/-/tree/main/packages/service",
  "scripts": {
    "prebuild": "rimraf .next/ && mkdir .next",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev --port=${LOCAL_PORT:-9955}",
    "lint": "eslint --ext .ts,.tsx,.js src/**",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start",
    "test": "vitest",
    "test:ci": "COLOR=false vitest run --coverage --no-color",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "validate": "pnpm lint && pnpm typecheck && pnpm test",
    "test:e2e": "playwright test \"$@\"",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:merge-reports": "playwright merge-reports \"$@\""
  },
  "browserslist": [
    "extends @zapier/browserslist-config-zapier"
  ],
  "dependencies": {
    "@datadog/browser-rum": "^5.11.0",
    "@sentry/integrations": "^7.107.0",
    "@sentry/react": "^7.107.0",
    "@splitsoftware/splitio": "^10.27.0",
    "@splitsoftware/splitio-react": "^1.11.1",
    "@types/node": "^20.12.12",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "@zapier/avro-events": "^1.11.46",
    "@zapier/design-system": "^9.0.0",
    "@zapier/design-system-beta": "^2.0.0",
    "@zapier/design-tokens": "^1.8.0",
    "@zapier/events-toolkit-browser": "^1.1.13",
    "@zapier/identity": "^2.0.0",
    "@zapier/marketing-site-layout": "^2.0.0",
    "@zapier/react-fullstory": "^1.8.0",
    "@zapier/universal-layout": "^12.0.0",
    "@zapier/zinnia-icons": "^0.3.0",
    "http-proxy-middleware": "^2.0.6",
    "js-cookie": "~3.0.5",
    "next": "^14.1.3",
    "react": "^18.2.0",
    "react-aria": "^3.39.0",
    "react-aria-components": "^1.8.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.3",
    "@storybook/react": "8.6.12",
    "@playwright/test": "^1.50.1",
    "@testing-library/react": "^14.2.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@vitejs/plugin-react": "^4.3.4",
    "@zapier/browserslist-config-zapier": "^1.0.2",
    "@zapier/eslint-config-custom": "1.0.0",
    "git-revision-webpack-plugin": "~5.0.0",
    "jsdom": "^25.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2",
    "vitest": "^2.0.5",
    "ts-node": "^10.9.1"
  },
  "sideEffects": false,
  "bin": {
    "wait-for-vercel": "./scripts/wait-for-vercel.js"
  }
}
